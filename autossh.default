# Default settings for autossh.  This file is sourced by /bin/bash
# from /etc/init.d/autossh.

AUTOSSH_USER=autossh

AUTOSSH_OPTS='-M 0 -q -N -f -o ServerAliveCountMax=3'
AUTOSSH_OPTS+=' -o ServerAliveInterval=60'

